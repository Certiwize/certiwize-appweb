    "feature_7_benefit_2": "Digital signature",
    "feature_7_benefit_3": "Automatic archiving",
    "feature_7_benefit_4": "Proof of attendance",
    "feature_8_title": "Hot Questionnaire",
    "feature_8_desc": "Automatic sending of hot satisfaction questionnaire at the end of training. Responses are collected and analyzed to continuously improve the quality of your training.",
    "feature_8_benefit_1": "Immediate sending",
    "feature_8_benefit_2": "High response rate",
    "feature_8_benefit_3": "Automatic analysis",
    "feature_8_benefit_4": "Continuous improvement",
    "feature_9_title": "Invoice Generation",
    "feature_9_desc": "Automatically create your invoices from training data. Automatic amount calculation, sequential numbering and compliance with legal mentions. PDF export and integrated accounting.",
    "feature_9_benefit_1": "Automatic calculation",
    "feature_9_benefit_2": "Tax compliance",
    "feature_9_benefit_3": "Automatic numbering",
    "feature_9_benefit_4": "Multi-format export",
    "feature_10_title": "Hot Questionnaire Follow-up",
